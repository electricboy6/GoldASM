#include lib/serial_out.gasm

.org %0200
lda #E9 // 233, largest fib number we can fit
cpa 07
// previous term
lda #00
cpa 00
// constant to add to numbers to convert to ascii
lda #30
cpa 02

lda #01
cpa 01
lda #00
LOOP:
    add 00, 01
    cpa 06
    cpr 01
    cpa 00
    cpr 06
    cpa 01
    bne 07, ~LOOP

jmp ~END

// program is over, just loop forever
.org %FFF8
END:
    noop
    jmp ~END
// reset vector
.org %FFFC
.word #02
.word #00
// dependencies
.org