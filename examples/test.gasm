#include lib/2byte_to_ascii.gasm
#define RESETVEC %FFFC

.org %0200
// load the divisor
lda #A8
sta %00
lda #94
sta %01

// load the dividend
lda #0a
sta %10

// divide
jsr ~2byte_to_ascii.DIV_16_7

// show the output
lda %11
cpa 07
lda %20
cpa 04
lda %21
cpa 05

//lda #a8
//sta %80
//lda #94
//sta %81
//jsr ~2byte_to_ascii.PRINT_2_BYTES

jmp ~END

// program is over, just loop forever
.org %FFF8
END:
    noop
    jmp ~END
.org *RESETVEC
.word #02
.word #00

.org